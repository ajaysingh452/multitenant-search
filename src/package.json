{
  "name": "search-router",
  "version": "1.0.0",
  "description": "Lightning-fast multi-tenant search router",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "node --loader tsx/esm src/index.ts",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --collectCoverage",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "benchmark": "node dist/benchmark/load-test.js"
  },
  "dependencies": {
    "@opensearch-project/opensearch": "^2.4.0",
    "typesense": "^1.7.2",
    "fastify": "^4.24.3",
    "@fastify/helmet": "^11.1.1",
    "@fastify/cors": "^8.4.0",
    "@fastify/rate-limit": "^9.0.1",
    "@fastify/jwt": "^7.2.4",
    "ioredis": "^5.3.2",
    "pg": "^8.11.3",
    "kafkajs": "^2.2.4",
    "pino": "^8.16.2",
    "pino-pretty": "^10.2.3",
    "@opentelemetry/api": "^1.7.0",
    "@opentelemetry/sdk-node": "^0.45.0",
    "@opentelemetry/exporter-jaeger": "^1.17.0",
    "ajv": "^8.12.0",
    "ajv-formats": "^2.1.1",
    "lru-cache": "^10.0.2",
    "uuid": "^9.0.1",
    "ms": "^2.1.3"
