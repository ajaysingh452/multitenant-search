  "devDependencies": {
    "@types/node": "^20.9.0",
    "@types/pg": "^8.10.7",
    "@types/uuid": "^9.0.7",
    "@types/ms": "^0.7.34",
    "@types/jest": "^29.5.8",
    "typescript": "^5.2.2",
    "tsx": "^4.6.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "eslint": "^8.54.0",
    "autocannon": "^7.12.0",
    "clinic": "^12.1.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "search",
    "multi-tenant",
    "elasticsearch",
    "opensearch",
    "typesense",
    "fastify"
